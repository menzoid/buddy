<fieldset class="form-group check_boxes optional snippet_intentions">
  <legend class="col-form-label pt-0">Intentions</legend>
  <input type="hidden" name="snippet[intention_ids][]" value="">
  <div class="intention-item">
    <input class="form-check-input check_boxes optional intention-selector" type="checkbox" value="1" name="snippet[intention_ids][]" id="snippet_intention_ids_1">
    <label class="form-check-label collection_check_boxes" for="snippet_intention_ids_1">Experience</label>
    <input class="form-check-input check_boxes optional intention-selector" type="checkbox" value="2" name="snippet[intention_ids][]" id="snippet_intention_ids_2">
    <label class="form-check-label collection_check_boxes" for="snippet_intention_ids_2">Party</label>
    <input class="form-check-input check_boxes optional intention-selector" type="checkbox" value="3" name="snippet[intention_ids][]" id="snippet_intention_ids_3">
