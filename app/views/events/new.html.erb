      <div class="container">
        <h4>Sky is the limit, create your event!</h4>
        <%= simple_form_for @event do |f| %>
          <%= f.input :name %>
          <%= f.input :description %>
          <%= f.input :start_time %>
          <%= f.input :photo, as: :file %>
          <%= f.submit class: "btn btn-create"%>
        <% end %>
      </div>
